class Person
{
int id ;
String name ;
Person ( int id , String name )
{
this.id = id ;
this.name = name ;
}
public String toString ()
{
	
	System.out.println ( id );
	return id+ " " + "name : " + name ;
}

}
 class Demo90
 {
 public static void main ( String [] args )
 {
 Person p1 = new Person (1 , " garima jain :");
 System.out.println (p1 );
 
 }
 
 }
