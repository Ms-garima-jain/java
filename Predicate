import java.util.*;
import java .util.function.*;
class Demo1
{
public static void main ( String [] args )
{
Predicate < String > p = x -> x.contains ("@")&& x.endsWith(".com") ;
System.out.println ( p.test ( "garima@gmail.com" ));
System.out.println (p.test ("gar@gmail"));
System.out.println ( p.test ( "garima " ));

}

}
