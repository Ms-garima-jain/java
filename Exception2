String x = args [0] ;
System.out.println ( " first " );
String y = args [1] ;
System.out.println ( " second " );
int a = Integer.parseInt (x);
System.out.println ( " third " );
int b = Integer.parseInt (y );
 System.out.println ( " fourt " );
 
 int c =a/b ;
 System.out.println ( " done " );

}
catch ( ArithmeticException obj )
{
