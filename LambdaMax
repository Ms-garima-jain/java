@FunctionalInterface
 interface  Maximum
 {
 public int max ( int a , int b , int c );
 
 }
  class Main 
  {
  public static void main ( String [] args )
  {
  
  Maximum max = (a, b, c ) -> {
  if ( a> b && a> c )  
  return a ; 
  else  if ( b > a && b > c)
  {
  return b ;
  }
  else
  {
  return c ;
  }
   
  };
 System.out.println ( max.max ( 1000, 200, 30 ));  
  }
  
  }
